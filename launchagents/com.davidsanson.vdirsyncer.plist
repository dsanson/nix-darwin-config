<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.davidsanson.vdirsyncer</string>
  <key>ProgramArguments</key>
  <array>
      <string>/etc/profiles/per-user/desanso/bin/vdirsyncer</string>
      <string>sync</string>
  </array>
  <key>EnvironmentVariables</key>
  <dict>
      <key>PATH</key>
      <string>$HOME/.nix-profile/bin:/etc/profiles/per-user/$USER/bin:/run/current-system/sw/bin:/nix/var/nix/profiles/default/bin:/usr/local/bin:/usr/bin:/usr/sbin:/bin:/sbin</string>
  </dict>
  <key>StartInterval</key>
  <integer>30</integer>
  <key>Nice</key>
  <integer>10</integer>
  <key>StandardOutPath</key>
  <string>/tmp/com.davidsanson.vdirsyncer.out</string>
  <key>StandardErrorPath</key>
  <string>/tmp/com.davidsanson.vdirsyncer.err</string>
</dict>
</plist>
